{
	"name": "gameboy-web-ui",
	"version": "0.0.1",
	"private": true,
	"scripts": {
    "dev:wasm": "wasm-pack build ./../emulator/gameboy --out-dir ../../web-ui/src/lib/wasm --out-name gameboy --target web",
		"dev": "npm run dev:wasm && vite dev",
    "build:wasm": "~/.cargo/bin/wasm-pack build ./../emulator/gameboy --out-dir ../../web-ui/src/lib/wasm --out-name gameboy --target web",
		"build": "./deploy/build1.sh && ./deploy/build2.sh && npm run build:wasm && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0"
	},
	"type": "module"
}
